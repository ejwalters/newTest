
<form>
		<fieldset>
			<legend>Exercise</legend>
			<p>Name: <input id="name" type="text" name="name" value="{{name}}"/></p>
			<p>Reps: <input  id="reps" type="text" name="reps" value="{{reps}}"/></p>
			<p>Weight: <input id="weight" type="text" name="weight" value="{{weight}}"/></p>
			<p>Date: <input id="date" type="text" name="date" value="{{date}}" /></p>
			<p>Lbs: <input id="lbs" type="text" name="lbs" value="{{lbs}}"/></p>
			
		</fieldset>
		<input id="addExercise" type="submit" name="add" value="Add Exercise" onclick="readInput(this.form)" />
	</form>